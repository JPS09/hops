<%= render "shared/searchbar", search: searches_path %>

<div class="card">
  <% @results.each do |result| %>
    <% search_res = result.searchable %>
    <% if search_res.class.name == "Beer" %>
      <%= link_to beer_path(search_res.id), class: 'beer-card-lg-link' do  %>
        <%= render "shared/beer-cards/beer-card-lg", beer: search_res %>
      <% end %>
    <% end %>
  <% end %>
</div>
